{
    "name": "nz/wordpress-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Nz Wordpress bundle with sonata integration",
    "keywords": ["sonata", "wordpress"],
    "homepage": "https://nzlab.es",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Zenith",
            "email": "2cb.md2@gmail.com",
            "homepage": "https://nzlab.es"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/formatter-bundle": "~2.2",
        "sonata-project/datagrid-bundle": "~2.2",
        "sonata-project/seo-bundle": "^1.1.5",
        "aws/aws-sdk-php": "~2.7",
        "jackalope/jackalope-doctrine-dbal": "~1.1",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "friendsofsymfony/rest-bundle": "~1.1",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nz\\WordpressBundle\\": ""
        }
    }
}
